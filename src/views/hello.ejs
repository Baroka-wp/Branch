<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="src/style/hello.css" />
    <!-- <link rel="stylesheet" href="normalize.css" /> -->
    <title>Applicants list</title>
  </head>
  <body>

    <!-- Header Starts -->
    <div class="header">
      <div class="header__left">
        <h1>Branch</h1>
      </div>


        <h1>Applicants list<span class="material-icons"> arrow_drop_down </span></h1>

      <div class="header__right">
        <span class="new"> New project + </span>
        <span class="material-icons"> notifications_none</span>
        <span class="material-icons"> search </span>
      </div>
    </div>
    <!-- Header Ends -->

    <!-- Main Body Starts -->
    <div class="main__body">
      <!-- Sidebar Starts -->
      <div class="sidebar">
        <div class="sidebarOption">
          <span class="material-icons"> dashboard </span>
          <h3>Dashboard</h3>
        </div>

        <div class="sidebarOption">
          <span class="material-icons"> domain </span>
          <h3>Available units</h3>
        </div>

        <div class="sidebarOption">
          <span class="material-icons"> security </span>
          <h3>Invoices</h3>
        </div>

        <div class="sidebarOption sidebarOption__active">
          <span class="material-icons"> access_time </span>
          <h3 class="project">Project</h3>
        </div>

        <div class="sidebarOption">
          <span class="material-icons"> share </span>
          <h3>Social media</h3>
        </div>

        <div class="sidebarOption">
          <span class="material-icons"> note </span>
          <h3>Drafts</h3>
        </div>

        <div class="sidebarOption">
          <span class="material-icons"> bookmark_border </span>
          <h3>Documentation</h3>
        </div>
        <br>
        <div class="sidebar__footer">
          <div class="sidebar__footerIcons">
            <div class="sidebarOption">
              <span class="material-icons"> settings </span>
              <h3>Settings</h3>
            </div>
            <div class="sidebarOption">
              <span class="material-icons"> help_outline </span>
              <h3>Help Center</h3>
            </div>
            <div class="sidebarOption">
              <p class="image"><p>CHOUMAN</p><br>
            </div>
          </div>
        </div>
      </div>
      <!-- Sidebar Ends -->

      <!-- App List Starts -->
      <div class="AppList">
        <!-- Settings Starts -->
        <div class="AppList__settings">
          <div class="AppList__settingsLeft">
             <button class="sidebar__compose">Filter Project<span class="material-icons"> filter_list </span></button><span class="space"></span>
            <button class="sidebar__compose">Sort Project<span class="material-icons"> sort </span></button><span class="space"></span>
            <button class="sidebar__compose"><input type="text" placeholder="African Samurai" /><span class="material-icons">search</span><span class="space"></span>
          </div>
          <div class="AppList__settingsLeft">
            <button class="sidebar__compose">Refresh<span class="material-icons"> rotate_right </span></button><span class="space"></span>
            <button class="sidebar__compose">Customize<span class="material-icons"> settings </span></button><span class="space"></span>
            <button class="sidebar__compose"><span class="material-icons"> more_vert </span></button>
          </div>
        </div>
        <!-- Settings Ends -->

        <!-- Section Starts -->
        <div class="AppList__sections">
          <div class="section section__selected">
            <input type="checkbox" name="" id="" />
            <h4>Adress E-mail</h4><span class="material-icons"></span>
          </div>

          <div class="section">
            <h4>Nom Et Prenoms</h4><span class="material-icons"></span>
          </div>

          <div class="section">
            <h4>Age</h4>
          </div>
          <div class="section">
            <h4>Sexe</h4>
          </div>
          <div class="section">
            <h4>Nationalite</h4>
          </div>
        </div>
        <% if(data.length){

        for(var i = 0; i< data.length; i++) {%>
        <div class="AppList__list">
          <!-- App Row Starts -->
          <div class="AppRow">
            <div class="AppRow__options">
              <input type="checkbox" name="" id="" />
            </div>


            <div class="AppRow__student">
              <span><%= data[i].email%></span>
            </div>

            <span class="AppRow__description"> <%= data[i].name%> </span>
            <span class="AppRow__description"> <%= data[i].age%></span>
            <span class="AppRow__description"><%= data[i].sexe%></span>
            <span class="AppRow__description"><%= data[i].nationality%></span>

          </div>
          <% }

           }else{ %>
                <h6 colspan="3">No user</h6>
          <% } %>
      </div>
      <% if (pages > 0) { %>
                <ul class="pagination text-center">
                    <% if (current == 1) { %>
                        <li class="disabled"><a class="btn">«</a></li>
                    <% } else { %>
                        <li><a href="/dashboard/1" class="btn btn-primary">«</a></li>
                    <% } %>
                    <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                    <% if (i !== 1) { %>
                        <li class="disabled"><a  class="btn">...</a></li>
                    <% } %>
                    <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                        <% if (i == current) { %>
                            <li class="active"><a  class="btn"><%= i %></a></li>
                        <% } else { %>
                            <li><a href="/dashboard/<%= i %>" class="btn"><%= i %></a></li>
                        <% } %>
                        <% if (i == Number(current) + 4 && i < pages) { %>
                            <li class="disabled"><a  class="btn">...</a></li>
                        <% } %>
                    <% } %>
                    <% if (current == pages) { %>
                        <li class="disabled"><a class="btn">»</a></li>
                    <% } else { %>
                        <li><a href="/dashboard/<%= pages %>" class="btn btn-primary">»</a></li>
                    <% } %>
                </ul>
            <% } %>
  </body>
</html>
